<?xml version="1.0" encoding="UTF-8"?>
<progress_configs>
 	<command sn="0" path="usb_spl.bin" address="0xd9000000" run="true">load spl</command>
 	<command sn="1" path="u-boot-orig-usid.bin" address="0x8f800000" run="false">load to ram</command>
 	<command sn="2">dcache off</command>
	<command sn="3">icache off</command>	
	<command sn="4">run 0x8f800000</command>
	<command sn="5">burn_efuse</command>
	<command sn="6">burn_boardid</command>
	<command sn="7">dcache on</command>
	<command sn="8">icache on</command>
	<command sn="9" path="u-boot-orig.bin" address="0x8f800000" run="false">load uboot</command>
	<command sn="10">run 0x8f800000</command>
	<command sn="11">nand erase 0</command>
	<command sn="12">defenv</command>
	<command sn="13">set_chgcur 0</command>
	<command sn="14">video dev bl_off</command>
	<command sn="15">video dev disable</command>
	<command sn="16">setenv bootargs ${bootargs} a9_clk_max=600000000</command>
	<command sn="17" cmd="setenv recovery_command --usb_burning" address="0x82000000">load addr</command>
	<command sn="18">cmd_in_mem 0x82000000</command>
	<command sn="19">save</command>
	<command sn="20" path="usb_burning.img" address="0x82000000" run="false">load uimage</command>
	<command sn="21" count="100">bootm 0x82000000</command>
	<command sn="22">wipe_data</command>
	<command sn="23">copy_media</command>
	<command sn="24" count="150">update:update.zip</command>	
	<command sn="25">shutdown</command>
